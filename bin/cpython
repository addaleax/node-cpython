#!/usr/bin/env node

var argv = process.argv
console.log(argv);

parse(argv)

function parse(arr) {
  console.log(flags(arr));
}

function flags(arr) {
  var flags = []
  arr.forEach(function(el) {
    var arg = el.trim()
    var f = arg.match(/(\-\-)\w+/g)
    if (f) {
      flags = Array.prototype.concat(flags, f)
    }
  })
  return flags
}

function description() {

}
